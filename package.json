{
  "name": "flat-diff",
  "version": "0.0.0",
  "description": "A naive diff algorithm based on the unique key path of each value in an Object / JSON",
  "main": "lib/index.js",
  "repository": "git@github.com:yeojz/flat-diff.git",
  "author": "Gerald Yeo <contact@fusedthought.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf lib coverage",
    "coverage": "jest --coverage",
    "coverage:upload": "codecov",
    "lint": "eslint --ext js src",
    "test": "jest"
  },
  "keywords": [
    "diff",
    "json",
    "key",
    "path",
    "changeset",
    "object",
    "keypath"
  ],
  "jest": {
    "coverageDirectory": "./coverage/"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^2.2.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.0.0",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0"
  },
  "dependencies": {
    "flat": "^2.0.1"
  }
}
